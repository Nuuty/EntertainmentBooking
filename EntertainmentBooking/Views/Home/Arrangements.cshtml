@model IEnumerable<EntertainmentBooking.Models.arrangement>
@{
    ViewBag.Title = "Arrangements";
}

<div class="jumbotron">
    @{
        foreach (var arrangement in Model)
        {
            <h1>@arrangement.name</h1>
            foreach (var band in arrangement.bands)
            {
                <h2>@band.name</h2>
            }
        }
    }
</div>